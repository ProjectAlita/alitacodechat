{
  "name": "alitacodechat",
  "displayName": "Elitea Code Chat",
  "version": "0.0.10",
  "publisher": "ProjectAlita",
  "icon": "images/icon.png",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "webpack",
    "build": "webpack --env mode=$NODE_ENV --devtool hidden-source-map",
    "build:dev": "webpack --env mode=development",
    "watch": "webpack --env mode=development --env watch=true",
    "lint": "eslint src --ext js  --max-warnings 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "shared": "^0.0.1",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}
